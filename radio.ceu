native _radioBegin();

input (int, float, float, int) RADIO_RX; // id, latitude, longitude, heading

native do
  void radioBegin() {
    Serial.println("Radio start.");
  }
end

_radioBegin();

loop do
  // radio stuff

  async do
    var int id, heading;
    var float latitude, longitude;

    emit RADIO_RX => (id, latitude, longitude, heading);
  end

  await 10s;
end